<div id="app">React has not yet rendered</div>
<%#= react_component 'MainApp' %>
<!-- <script src="node_modules/react/dist/react.js"></script>
<script src="node_modules/react-dom/dist/react-dom.js"></script> -->

<% if @rental %>
  <% @rental.errors.full_messages.each do |error| %>
    <%= error %>
  <% end %>
<% end %>

<%= form_for :rental, url: rentals_path do |f| %>
  <div class="form_field" >
	  <%= label(:rental, :title) %>
    <%= f.text_field :title %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :property_type) %>
	  <%= f.text_field :property_type %>
  </div>

  <div class="form_field">
    <%= label(:rental, :address) %>
	  <%= f.text_field :address %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :unit) %>
	  <%= f.text_field :unit %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :price) %>
	  <%= f.text_field :price %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :beds) %>
	  <%= f.text_field :beds %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :baths) %>
	  <%= f.text_field :baths %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :pets) %>
	  <%= f.text_field :pets %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :parking) %>
	  <%= f.text_field :parking %>
  </div>

  <div class="form_field" >
    <%= label(:rental, :description) %>
	  <%= f.text_field :description %>
  </div>

  <div class="form_submit_button" >
	  <%= f.submit "Post a rental" %>
  </div>

<% end %>